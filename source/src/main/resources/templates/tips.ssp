<html>
<head>
    <title>tips</title>
</head>
<body>
<% import com.fishstory.oopsday.domain.tip.Tip%>
<%@ var tips:List[Tip] %>
    #for(tip <- tips)
<ul class="tip">
    <li class="tip_title">${tip.title}</li>
    <li class="tip_content">${tip.content}</li>
    <li class="tip_author">${tip.author}</li>
</ul>
#end
</body>
</html>